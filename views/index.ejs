    <!-- Start Appointment -->
    <section class="appointment">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>
                            <%= __("index.appointment.title") %>
                        </h2>
                        <img src="img/section-img.png" alt="#">
                        <p>
                            <%= __("index.appointment.description") %>
                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-md-12 col-12">
                    <form class="form" action="#">
                        <div class="row">
                            <div class="col-lg-6 col-md-6 col-12">
                                <div class="form-group">
                                    <input name="name" type="text" placeholder="<%= __("index.appointment.form.name")
                                        %>">
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-12">
                                <div class="form-group">
                                    <input name="email" type="email" placeholder="<%= __("index.appointment.form.email") %>">
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-12">
                                <div class="form-group">
                                    <input name="phone" type="text" placeholder="<%= __("index.appointment.form.phone")
                                        %>">
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-12">
                                <div class="form-group">
                                    <div class="nice-select form-control wide" tabindex="0">
                                        <span class="current">
                                            <%= __("index.appointment.form.department") %>
                                        </span>
                                        <ul class="list">
                                            <li data-value="1" class="option selected">
                                                <%= __("index.appointment.form.department") %>
                                            </li>
                                            <li data-value="2" class="option">
                                                <%= __("index.appointment.form.department_option1") %>
                                            </li>
                                            <li data-value="3" class="option">
                                                <%= __("index.appointment.form.department_option2") %>
                                            </li>
                                            <li data-value="4" class="option">
                                                <%= __("index.appointment.form.department_option3") %>
                                            </li>
                                            <li data-value="5" class="option">
                                                <%= __("index.appointment.form.department_option4") %>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-12">
                                <div class="form-group">
                                    <div class="nice-select form-control wide" tabindex="0">
                                        <span class="current">
                                            <%= __("index.appointment.form.doctor") %>
                                        </span>
                                        <ul class="list">
                                            <li data-value="1" class="option selected">
                                                <%= __("index.appointment.form.doctor") %>
                                            </li>
                                            <li data-value="2" class="option">
                                                <%= __("index.appointment.form.doctor_option1") %>
                                            </li>
                                            <li data-value="3" class="option">
                                                <%= __("index.appointment.form.doctor_option2") %>
                                            </li>
                                            <li data-value="4" class="option">
                                                <%= __("index.appointment.form.doctor_option3") %>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-12">
                                <div class="form-group">
                                    <input type="text" placeholder="<%= __("index.appointment.form.date") %>"
                                    id="datepicker">
                                </div>
                            </div>
                            <div class="col-lg-12 col-md-12 col-12">
                                <div class="form-group">
                                    <textarea name="message" placeholder="<%= __("index.appointment.form.message")
                                        %>"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-5 col-md-4 col-12">
                                <div class="form-group">
                                    <div class="button">
                                        <button type="submit" class="btn">
                                            <%= __("index.appointment.form.submit") %>
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-7 col-md-8 col-12">
                                <p>
                                    <%= __("index.appointment.form.confirmation") %>
                                </p>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="col-lg-6 col-md-12">
                    <div class="appointment-image">
                        <img src="img/img-contact.jpg" alt="#">
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End Appointment -->

    <!-- Start Newsletter Area -->
    <section class="newsletter section">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-12">
                    <div class="subscribe-text">
                        <h6>
                            <%= __("index.newsletter.title") %>
                        </h6>
                        <p>
                            <%= __("index.newsletter.description") %>
                        </p>
                    </div>
                </div>
                <div class="col-lg-6 col-12">
                    <div class="subscribe-form">
                        <form action="mail/mail.php" method="get" target="_blank" class="newsletter-inner">
                            <input name="EMAIL" placeholder="<%= __("index.newsletter.form.email") %>"
                            class="common-input"
                            onfocus="this.placeholder = ''" onblur="this.placeholder = '<%=
                                __("index.newsletter.form.email") %>'"
                                required type="email">
                                <button class="btn">
                                    <%= __("index.newsletter.form.submit") %>
                                </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- /End Newsletter Area -->
