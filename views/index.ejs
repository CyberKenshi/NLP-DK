        <div class="hero-slider">
            <!-- Start Single Slider -->
            <div class="single-slider" style="background-image:url('img/lab/lab4.jpg')">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-7">
                            <div class="text">
                                <h1>
                                    <%= __("index.slider1.title") %> <span>
                                            <%= __("index.slider1.highlight") %>
                                        </span>
                                        <%= __("index.slider1.subtitle") %>
                                </h1>
                                <div class="button">
                                    <a href="/about" class="btn">
                                        <%= __("header.about_us") %>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Single Slider -->
            <!-- Start Single Slider -->
            <div class="single-slider" style="background-image:url('img//lab/congbo.jpg')">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-7">
                            <div class="text">
                                <h1>
                                    <%= __("index.slider2.title") %> <span>
                                            <%= __("index.slider2.highlight1") %>
                                        </span>
                                        <%= __("index.slider2.subtitle") %> <span>
                                                <%= __("index.slider2.highlight2") %>
                                            </span>
                                </h1>
                                <div class="button">
                                    <a href="/projects" class="btn">
                                        <%= __("index.slider2.button") %>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Single Slider -->
            <!-- Start Single Slider -->
            <div class="single-slider" style="background-image:url('img/lab/lab2.jpg')">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-7">
                            <div class="text">
                                <h1>
                                    <%= __("index.slider3.title") %> <span>
                                            <%= __("index.slider3.highlight1") %>
                                        </span>
                                        <%= __("index.slider3.subtitle") %> <span>
                                                <%= __("index.slider3.highlight2") %>
                                            </span>
                                </h1>
                                <div class="button">
                                    <a href="/contact" class="btn">
                                        <%= __("header.contact") %>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Single Slider -->
        </div>
    </section>
    <!--/ End Slider Area -->

    <!-- Start Schedule Area -->
    <section class="schedule">
        <div class="container">
            <div class="schedule-inner">
                <div class="row">
                    <div class="col-lg-4 col-md-6 col-12">
                        <!-- single-schedule -->
                        <div class="single-schedule first">
                            <div class="inner">
                                <div class="icon">
                                    <i class="fa fa-ambulance"></i>
                                </div>
                                <div class="single-content">
                                    <span>
                                        <%= __("index.schedule1.tag") %>
                                    </span>
                                    <h4>
                                        <%= __("index.schedule1.title") %>
                                    </h4>
                                    <p>
                                        <%= __("index.schedule1.description") %>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6 col-12">
                        <!-- single-schedule -->
                        <div class="single-schedule middle">
                            <div class="inner">
                                <div class="icon">
                                    <i class="icofont-prescription"></i>
                                </div>
                                <div class="single-content">
                                    <span>
                                        <%= __("index.schedule2.tag") %>
                                    </span>
                                    <h4>
                                        <%= __("index.schedule2.title") %>
                                    </h4>
                                    <p>
                                        <%= __("index.schedule2.description") %>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-12 col-12">
                        <!-- single-schedule -->
                        <div class="single-schedule last">
                            <div class="inner">
                                <div class="icon">
                                    <i class="icofont-ui-clock"></i>
                                </div>
                                <div class="single-content">
                                    <span>
                                        <%= __("index.schedule3.tag") %>
                                    </span>
                                    <h4>
                                        <%= __("index.schedule3.title") %>
                                    </h4>
                                    <ul class="time-sidual">
                                        <li class="day">
                                            <%= __("index.schedule3.time1.day") %> <span>
                                                    <%= __("index.schedule3.time1.hours") %>
                                                </span>
                                        </li>
                                        <li class="day">
                                            <%= __("index.schedule3.time2.day") %> <span>
                                                    <%= __("index.schedule3.time2.hours") %>
                                                </span>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--/End Start schedule Area -->

    <!-- Start Fun-facts -->
    <div id="fun-facts" class="fun-facts section overlay">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-6 col-12">
                    <div class="single-fun">
                        <i class="icofont-folder-open"></i>
                        <div class="content">
                            <span class="counter">40</span>
                            <p>
                                <%= __("index.funfacts.projects") %>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-12">
                    <div class="single-fun">
                        <i class="icofont icofont-user-alt-3"></i>
                        <div class="content">
                            <span class="counter">20</span>
                            <p>
                                <%= __("index.funfacts.personnel") %>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-12">
                    <div class="single-fun">
                        <i class="icofont-newspaper"></i>
                        <div class="content">
                            <span class="counter">150</span>
                            <p>
                                <%= __("index.funfacts.papers") %>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-12">
                    <div class="single-fun">
                        <i class="icofont icofont-table"></i>
                        <div class="content">
                            <span class="counter">10</span>
                            <p>
                                <%= __("index.funfacts.experience") %>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--/ End Fun-facts -->

    <!-- Start Why choose -->
    <section class="why-choose section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>
                            <%= __("index.why_choose.title") %>
                        </h2>
                        <img src="img/section-img.png" alt="#">
                        <p>
                            <%= __("index.why_choose.description") %>
                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-12">
                    <div class="choose-left">
                        <h3>
                            <%= __("index.why_choose.subtitle") %>
                        </h3>
                        <p>
                            <%= __("index.why_choose.paragraph1") %>
                        </p>
                        <p>
                            <%= __("index.why_choose.paragraph2") %>
                        </p>
                        <div class="row">
                            <div class="col-lg-6">
                                <ul class="list">
                                    <li><i class="fa fa-caret-right"></i>
                                        <%= __("index.why_choose.list1.item1") %>
                                    </li>
                                    <li><i class="fa fa-caret-right"></i>
                                        <%= __("index.why_choose.list1.item2") %>
                                    </li>
                                    <li><i class="fa fa-caret-right"></i>
                                        <%= __("index.why_choose.list1.item3") %>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-lg-6">
                                <ul class="list">
                                    <li><i class="fa fa-caret-right"></i>
                                        <%= __("index.why_choose.list2.item1") %>
                                    </li>
                                    <li><i class="fa fa-caret-right"></i>
                                        <%= __("index.why_choose.list2.item2") %>
                                    </li>
                                    <li><i class="fa fa-caret-right"></i>
                                        <%= __("index.why_choose.list2.item3") %>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-12">
                    <div class="choose-right">
                        <div class="video-image">
                            <div class="promo-video">
                                <div class="waves-block">
                                    <div class="waves wave-1"></div>
                                    <div class="waves wave-2"></div>
                                    <div class="waves wave-3"></div>
                                </div>
                            </div>
                            <a href="https://www.youtube.com/watch?v=RFVXy6CRVR4"
                                class="video video-popup mfp-iframe"><i class="fa fa-play"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--/ End Why choose -->

    <!-- Start Call to action -->
